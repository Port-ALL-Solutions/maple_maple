<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>		
	
		<record id="maple_rating_tree_view" model="ir.ui.view">
			<field name="name">maple.rating.tree.view</field>
			<field name="model">stock.quant</field>
			<field name="arch" type="xml">
				<tree string="Maple Rating" editable="1" edit="1" create="0">
	                <field name="product_id" readonly="1"/>
	                <field name="location_id" readonly="1" invisible="1"/>
	                <field name="owner_id" groups="stock.group_tracking_owner" readonly="1"/>
	                <field name="container_serial"/>
	                <field name="tmp_owner" readonly="0" options='{"no_open": True,"no_create": 1, "no_create_edit": 1}'/>
	                <field name="tmp_material" readonly="0" options='{"no_open": True,"no_create": 1, "no_create_edit": 1}'/>
	                <field name="container_state" readonly="0" options='{"no_open": True,"no_create": 1, "no_create_edit": 1}'/>
	                <field name="container_total_weight" sum="1"/>
	                <field name="tmp_tare" sum="1"/>
	                <field name="controler" options='{"no_open": True,"no_create": 1, "no_create_edit": 1}'/>
	                <field name="maple_seal_no"/>
	                <field name="maple_flavor"/>
	                <field name="maple_flaw"/>
	                <field name="maple_light" sum="1"/>
	                <field name="maple_grade"/>
	                <field name="maple_brix" avg="1"/>
	                <field name="maple_adjust_weight" avg="1"/>
	                <field name="maple_adjust_price"/>
	                <!-- field name="location_dest_id"/ -->
	                <field name="product_code" readonly="1"/>
   				</tree>
			</field>
		</record>	
			
        <record model="ir.actions.act_window" id="maple_rating_list_action">
            <field name="name">Maple Rating</field>
            <field name="res_model">stock.quant</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form</field>
			<field name="view_id" ref="maple_rating_tree_view" /> 
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Can not create anything from here. </p>
            </field>
        </record>
	
	</data>
</odoo>
