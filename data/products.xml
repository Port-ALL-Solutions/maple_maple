<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="raw_maple" model="product.template">
            <field name="name">Raw Maple Syrup</field>				
            <field name="default_code">E</field>		
            <field name="type">product</field>
            <field name="maple_syrup">1</field>
            <field name="categ_id" ref="maple_category"/>		
		</record>	

		<record id="regular_maple" model="product.template">
            <field name="name">Regular Maple</field>				
            <field name="default_code">R</field>		
            <field name="type">product</field>
            <field name="maple_syrup">1</field>
            <field name="categ_id" ref="maple_category"/>		
		</record>	

		<record id="organic_maple" model="product.template">
            <field name="name">Organic Maple Syrup</field>				
            <field name="default_code">O</field>		
            <field name="type">product</field>
            <field name="maple_syrup">1</field>
            <field name="categ_id" ref="maple_category"/>		
		</record>	

		<record id="purchase_barrel" model="product.template">
            <field name="name">Maple Syrup Barel (to identified)</field>				
            <field name="default_code">B</field>		
            <field name="type">product</field>
            <field name="maple_syrup">1</field>
            <field name="sale_ok">0</field>
            <field name="purchase_ok">1</field>
            <field name="categ_id" ref="maple_category"/>		
		</record>	

		<record id="purchase_tote" model="product.template">
            <field name="name">Maple Syrup Tote (to identified)</field>				
            <field name="default_code">T</field>		
            <field name="type">product</field>
            <field name="maple_syrup">1</field>
            <field name="sale_ok">0</field>
            <field name="purchase_ok">1</field>
            <field name="categ_id" ref="maple_category"/>		
		</record>	


	</data>
</odoo>
