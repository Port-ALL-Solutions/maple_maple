<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
			
		<record model="ir.sequence" id="seq_daily_in">
   			<field name="name">Daily_In</field>
   			<field name="code">stock.daily_in</field>
   			<field name="prefix">%(year)s%(month)s%(day)s-</field>
   			<field name="padding">3</field>
   			<field name="use_date_range">True</field>
   			<field name="daily_date_range">True</field> 
		</record>

        <record id="ir_sequence_weighing_SE1" model="ir.sequence">
            <field name="name">SE1 weighing sequence</field>
            <field name="code">stock.location_weighing</field>
            <field name="implementation">standard</field>
            <field name="prefix">PES/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">False</field>
            <field name="daily_date_range">False</field>    
        </record>

        <record id="ir_sequence_class1_SE1" model="ir.sequence">
            <field name="name">SE1 class1 sequence</field>
            <field name="code">stock.location_class1</field>
            <field name="implementation">standard</field>
            <field name="prefix">CL1%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>    
        </record>

        <record id="ir_sequence_class2_SE1" model="ir.sequence">
            <field name="name">SE1 class2 sequence</field>
            <field name="code">stock.location_class2</field>
            <field name="implementation">standard</field>
            <field name="prefix">CL2%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>    
        </record>
        
        <record id="ir_sequence_class1_SE1_out" model="ir.sequence">
            <field name="name">SE1 class1 out sequence</field>
            <field name="code">stock.location_class1</field>
            <field name="implementation">standard</field>
            <field name="prefix">CL1/OUT/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>    
        </record>

        <record id="ir_sequence_class2_SE1_out" model="ir.sequence">
            <field name="name">SE1 class2 out sequence</field>
            <field name="code">stock.location_class2</field>
            <field name="implementation">standard</field>
            <field name="prefix">CL2/OUT/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>
        </record>
        
        <record id="ir_sequence_weighing_SENB" model="ir.sequence">
            <field name="name">SENB weighing sequence</field>
            <field name="code">stock.weighing_SENB</field>
            <field name="implementation">standard</field>
            <field name="prefix">PES/SENB/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">False</field>
            <field name="daily_date_range">False</field>
        </record>
        
        <record id="ir_sequence_class_SENB_in" model="ir.sequence">
            <field name="name">SENB class in sequence</field>
            <field name="code">stock.class_SENB_in</field>
            <field name="implementation">standard</field>
            <field name="prefix">CLNB/IN/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>
        </record>
        
        <record id="ir_sequence_class SENB_out" model="ir.sequence">
            <field name="name">SENB class out sequence</field>
            <field name="code">stock.class_SENB_out</field>
            <field name="implementation">standard</field>
            <field name="prefix">CLNB/OUT/%(year)s%(month)s%(day)s-</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
            <field name="active">1</field>
            <field name="use_date_range">True</field>
            <field name="daily_date_range">True</field>
        </record>
        
	</data>
</odoo>