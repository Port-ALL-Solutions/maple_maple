<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--#########################################################################
         Section 1: The below section specifies the paper format of the report you want 
         to create and its dimensions and orientation. 
         
        ##########################################################################--->

        <record id="paperformat_51x77mm" model="report.paperformat">
            <field name="name">Reception label ()Datamax DMX E-4203)</field>
            <field name="default" eval="True" />
            <field name="format">custom</field>
            <field name="page_height">51</field>
            <field name="page_width">77</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">2</field>
            <field name="margin_bottom">2</field>
            <field name="margin_left">2</field>
            <field name="margin_right">2</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">0</field>
            <field name="dpi">203</field>
        </record>

        <!--#########################################################################
        Section 2: In this section you specify the model you are creating this report for. in the
        name and file attributes write them in this format:

        the folder of your model.the name of the file of your report
         
        ##########################################################################--->

	    <report
        	id="reception_labels"
        	model="stock.picking"
        	string="Reception Labels"
        	report_type="qweb-pdf"
        	report_name="stock.reception_labels"
        	name="maple.reception_labels"
        	file="maple.reception_labels"
	    />
    
    
        <!--#########################################################################
        Section 3: This is the action record for the report. 
        for record "id" use the id of the report you put in section 2 
        for "ref" use the id of the paper format you put in section 1
         
        ##########################################################################--->    

            <record id="maple.maple_reception_labels" model="ir.actions.report.xml">
            	<field name="paperformat_id" ref="maple.paperformat_datamax_51x77mm" />
	        </record>

    </data>
</odoo>